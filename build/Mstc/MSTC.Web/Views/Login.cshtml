@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<Login>

@{
	Layout = "Master.cshtml";
}
@Html.Partial("~/Views/Partials/SectionHeader.cshtml")

<div class="section">
	<div class="container">
		<div class="row">
			<div class="col-sm-12">
				<p>
					Please use the form below to log in. If you don't have any log in details then you can <a href="@Model.Content.RegistrationPage.Url">register as a club member</a>.									
				</p>
			</div>
		</div>
		<div class="row">
			<div class="col-sm-12">
				@{ Html.RenderAction("RenderLogin", "Member"); }
			</div>
		</div>
	</div>
</div>
