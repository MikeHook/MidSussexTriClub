@inherits Umbraco.Web.Mvc.UmbracoViewPage<Mstc.Core.Domain.RegistrationDetails>
@{ 
    
}

<div class="form-group">
    @Html.LabelFor(m => m.PersonalDetails.FirstName, new { @class = "col-sm-3 control-label" })
    <div class="col-sm-7">
        @Html.TextBoxFor(m => m.PersonalDetails.FirstName, new { @class = "form-control", placeholder = "First name" })
    </div>
    <div class="col-sm-2"></div>
</div>
<div class="form-group">
    @Html.LabelFor(m => m.PersonalDetails.LastName, new { @class = "col-sm-3 control-label" })
    <div class="col-sm-7">
        @Html.TextBoxFor(m => m.PersonalDetails.LastName, new { @class = "form-control", placeholder = "Last name" })
    </div>
    <div class="col-sm-2"></div>
</div>    
