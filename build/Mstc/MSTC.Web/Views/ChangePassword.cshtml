@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.ChangePassword>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = "Master.cshtml";
}
@Html.Partial("~/Views/Partials/SectionHeader.cshtml")

<div class="section">
	<div class="container">
		<div class="row">
			<div class="col-sm-12">
				@Model.Content.BodyText
			</div>
		</div>
		<div class="row">
			<div class="col-sm-12">
				@{ Html.RenderAction("RenderChangePassword", "Member"); }
			</div>
		</div>
	</div>
</div>
